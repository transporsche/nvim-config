{
    "Figure-eric": {
        "prefix": "figure eric",
        "body": [
            "\\begin{figure}[htbp!]",
            "\t\\begin{center}",
            "\t\t\\includegraphics[width=\\textwidth]{$1}",
            "\t\\end{center}",
            "\t\\caption{$3}\\label{fig:$4}",
            "\\end{figure}",
            "$0"
        ],
        "description": "Figure mit voller Breitseite"
    },
    "lvcode-task": {
        "prefix": "lvcode-task",
        "body": ["\\textcolor{lvfarbetask}{\\mintinline{c}{$1}}"],
        "description": "inline code task"
    },
    "lvcode-double": {
        "prefix": "lvcode-double",
